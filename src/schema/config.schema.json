{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "required": [
    "input",
    "output"
  ],
  "properties": {
    "input": {
      "additionalProperties": false,
      "required": [
        "schema"
      ],
      "properties": {
        "schema": {
          "type": "string",
          "minLength": 1
        }
      }
    },
    "output": {
      "required": [
        "schema",
        "mapping",
        "type",
        "updateKey",
        "projectName"
      ],
      "properties": {
        "schema": {
          "type": "string",
          "minLength": 1
        },
        "mapping": {
          "type": "string",
          "minLength": 1
        },
        "type": {
          "type": "string",
          "enum": [
            "thng",
            "product",
            "place",
            "collection"
          ]
        },
        "updateKey": {
          "type": "string",
          "minLength": 1
        },
        "projectName": {
          "type": "string",
          "minLength": 1
        },
        "defaultRedirectUrl": {
          "type": "string"
        },
        "qrCodesOptions":{
          "type" : "string"
        }
      }
    }
  }
}